chantToneID = \relative c' {
  \clef "treble_8"
  \key c \major
  \set Score.timing = ##f

  <c e a>2 g'4 f <c e g>4. \divisioMaxima
  <c e g>2 a'4 <a, c f> e' <f, a d>4. \finalis
}

chantToneIf = \relative c' {
  \clef "treble_8"
  \key c \major
  \set Score.timing = ##f

  <c e a>2 g'4 f <c e g>4. \divisioMaxima
  <c e g>2 a'4 <a, c f>4. \finalis
}
