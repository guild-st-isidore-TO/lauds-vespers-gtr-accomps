chantToneVa = \relative c' {
  \clef "treble_8"
  \key d \major
  \set Score.timing = ##f

  <fs, a d>2 e'4 <d, g b>4. \divisioMaxima
  <cs e a>2 g'4 <d g b>4. \finalis 
}

chantToneVc = \relative c' {
  \clef "treble_8"
  \key d \major
  \set Score.timing = ##f

  <fs, a d>2 e'4 <fs, a d>4. \divisioMaxima
  <g b d>2 <e g b>4 cs' <fs, a d>4. \finalis 
}