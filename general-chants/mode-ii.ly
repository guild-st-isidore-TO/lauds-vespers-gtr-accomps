chantToneIIA = \relative c' {
  \clef "treble_8"
  \key e \major
  \set Score.timing = ##f

  <a cs e>2 ds4 <fs, a cs> <gs cs e>4. \divisioMaxima
  <a cs e>2 ds4 <e, gs cs>4. \finalis 
}

chantToneIID = \relative c' {
  \clef "treble_8"
  \key b \major
  \set Score.timing = ##f

  <gs b e>2 ds'4 <e, gs cs> <ds gs b>4. \divisioMaxima
  <e gs b>2 <e gs cs>4. \finalis 
}