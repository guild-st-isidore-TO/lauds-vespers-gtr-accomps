chantToneVIIIa = \relative c' {
  \clef "treble_8"
  \key f \major
  \set Score.timing = ##f

  <e, a c>2 <g bf d>4. \divisioMaxima
  <a c f>2 e'4 <g, bf d>4. \finalis 
}

chantToneVIIIg = \relative c' {
  \clef "treble_8"
  \key f \major
  \set Score.timing = ##f

  <a c f>2 e'4 <g, bf d> <e g c>4. \divisioMaxima
  <f a d>2 bf4 <g bf d> <e g c>4. \finalis 
}
