chantToneIVa = \relative c' {
  \clef "treble_8"
  \key g \major
  \set Score.timing = ##f

  <g b e>2 <fs a d>4. \divisioMaxima
  <g b d>2 b4 <fs b d> <g b e>4. \finalis 
}

chantToneIVd = \relative c' {
  \clef "treble_8"
  \key g \major
  \set Score.timing = ##f

  <g b e>2 <fs a d>4 e' <e, g b>4. \divisioMaxima
  <d fs b>2 <e a c>4 b' <d, fs a>4. \finalis 
}